{
  "name": "guitar-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/guitar-app/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{workspaceRoot}/dist/apps/guitar-app/web"],
      "options": {
        "outputPath": "dist/apps/guitar-app/web",
        "configFile": "apps/guitar-app/vite.config.ts"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "configFile": "apps/guitar-app/vite.config.ts"
      },
      "configurations": {
        "development": {
          "buildTarget": "guitar-app:build:development"
        },
        "production": {
          "buildTarget": "guitar-app:build:production"
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview",
      "options": {
        "configFile": "apps/guitar-app/vite.config.ts"
      },
      "configurations": {
        "development": {
          "buildTarget": "guitar-app:build:development"
        },
        "production": {
          "buildTarget": "guitar-app:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["apps/guitar-app/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/guitar-app"],
      "options": {
        "jestConfig": "apps/guitar-app/jest.config.cts"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "dependsOn": ["build:production"],
      "outputs": [],
      "options": {
        "cwd": "guitar-app",
        "commands": [
          "cp dist/apps/guitar-app/web/index.html dist/apps/guitar-app/web/404.html",
          "../node_modules/.bin/gh-pages -d dist/apps/guitar-app/web --branch gh-pages --dotfiles --dest guitar-app"
        ],
        "parallel": false
      }
    }
  }
}